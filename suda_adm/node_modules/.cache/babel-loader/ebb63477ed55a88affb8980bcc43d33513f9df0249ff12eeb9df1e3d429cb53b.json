    <el-input v-model.number=\"movieData.price\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"上映日期\" >\r\n          <el-date-picker\r\n              v-model=\"movieData.date\"\r\n              type=\"datetimerange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始日期\"\r\n              end-placeholder=\"结束日期\"\r\n              value-format=\"yyyy-MM-dd\">\r\n          </el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"影片介绍\" >\r\n          <el-input\r\n              type=\"textarea\"\r\n              autosize\r\n              placeholder=\"请输入内容\"\r\n              v-model=\"movieData.introduce\">\r\n          </el-input>\r\n        </el-form-item>\r\n\r\n        <!--          </el-form-item>-->\r\n        <el-form-item>\r\n          <el-button type=\"primary\" native-type=\"submit\">保存</el-button>\r\n          <el-button>取 消</el-button>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name: \"AddMovie\",\r\n  data(){\r\n    return{\r\n      movieData:{},\r\n    }\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}