{"ast":null,"code":"export default {\n  name: \"MovieCategory\",\n  props: {\n    id: {}\n  },\n  data() {\n    return {\n      model: {},\n      items: [],\n      categoryInfo: {},\n      form: {\n        name: ''\n      },\n      dialogFormVisible: false\n    };\n  },\n  methods: {\n    //保存新增分类\n    async save() {\n      console.log(\"开始执行save\");\n      //http://localhost:3000/admin/api/\n      const res = await this.$http.post('category', this.model); // eslint-disable-line no-unused-vars\n      //跳转路由\n      await this.$router.go(0);\n      this.$message({\n        type: 'success',\n        message: '保存成功'\n      });\n    },\n    //查找分类\n    async fetch() {\n      const res = await this.$http.get('/category');\n      this.items = res.data;\n    },\n    //编辑类别\n    categoryEdit(index, row) {\n      this.categoryInfo = row;\n      this.name = row.name;\n      this.dialogFormVisible = true;\n    },\n    async editSave() {\n      const res = await this.$http.put('/category');\n      console.log(res);\n    },\n    //删除类别\n    async remove(index, row) {\n      console.log(index, row._id, row.name);\n      this.$confirm(`此操作将删除分类\"${row.name}\", 是否继续?`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        const res = await this.$http.delete(`/category`, {\n          data: {\n            _id: row._id\n          }\n        }); // eslint-disable-line no-unused-vars\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        });\n        await this.$router.go(0);\n      });\n    }\n  },\n  //自动执行\n  created() {\n    this.fetch();\n  }\n};","map":{"version":3,"mappings":"AA4DA;EACAA;EACAC;IACAC;EACA;EACAC;IACA;MAEAC;MACAC;MACAC;MACAC;QACAP;MACA;MACAQ;IACA;EAGA;EACAC;IACA;IACA;MACAC;MACA;MACA;MACA;MACA;MACA;QACAC;QACAC;MACA;IACA;IACA;IACA;MACA;MACA;IACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACA;MACA;MACAH;IACA;IAEA;IACA;MACAA;MACA;QACAI;QACAC;QACAJ;MACA;QAEA;UAAAR;Y