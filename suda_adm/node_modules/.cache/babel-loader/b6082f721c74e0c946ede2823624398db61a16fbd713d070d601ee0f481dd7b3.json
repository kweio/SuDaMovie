{"ast":null,"code":"export default {\n  name: 'MovieList',\n  data() {\n    return {\n      // imageURL: '',\n      options: [],\n      movieInfo: [],\n      movieData: {},\n      dialogTitle: '添加影片',\n      dialogFormVisible: false,\n      form: {\n        // name: '',\n        // region: '',\n        // date1: '',\n        // date2: '',\n        // delivery: false,\n        // type: [],\n        // resource: '',\n        // desc: ''\n      }\n    };\n  },\n  methods: {\n    //获取分类信息\n    async categoryFetch() {\n      const res = await this.$http.get('/category');\n      this.options = res.data;\n    },\n    // 展示电影信息\n    async fetch() {\n      const res = await this.$http.get('/movie');\n      this.movieInfo = res.data;\n    },\n    // 新增电影信息\n    async save() {\n      this.movieData.category = this.movieData.category.join(\",\");\n      //http://localhost:3000/admin/api/\n      const res = await this.$http.post('/movie', this.movieData); // eslint-disable-line no-unused-vars\n      //跳转路由\n      await this.$router.go(0);\n      this.$message({\n        type: 'success',\n        message: '添加成功'\n      });\n    },\n    //编辑按钮事件\n    movieEdit(index, row) {\n      this.movieData = row;\n      this.name = row.name;\n      this.dialogFormVisible = true;\n    },\n    //删除电信信息\n    async remove(index, row) {\n      this.$confirm(`此操作将删除电影信息\"${row.name}\", 是否继续?`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        const res = await this.$http.delete(`/movie`, {\n          data: {\n            _id: row._id\n          }\n        }); // eslint-disable-line no-unused-vars\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        });\n        await this.$router.go(0);\n      });\n    },\n    //显示dialog\n    addMovie() {\n      this.dialogTitle = '添加电影';\n      this.dialogFormVisible = true;\n    },\n    handleAvatarSuccess(res) {\n      this.$set(this.movieData, 'icon', res.url);\n      this.movieData.icon = res.url;\n    },\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === 'image/jpeg';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n      return isJPG && isLt2M;\n    }\n  },\n  // 自动执行\n  created() {\n    this.fetch();\n  }\n};","map":{"version":3,"mappings":"AA8IA;EACAA;EACAC;IACA;MACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;QACA;QACA;QACA;QACA;QACA;QACA;QACA;QACA;MAAA;IAEA;EAGA;EACAC;IACA;IACA;MACA;MACA;IACA;IACA;IACA;MACA;MACA;IACA;IACA;IACA;MACA;MACA;MACA;MACA;MACA;MACA;QACAC;QACAC;MACA;IACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACA;IACA;MACA;QACAC;QACAC;QACAJ;MACA;QACA;UAAAR;YAAAa;UAAA;QAAA;QACA;UACAL;UACAC;QACA;QACA;MACA;IACA;IACA;IACAK;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;MACA;MAEA;QACA;MACA;MACA;QACA;MACA;MACA;IACA;EACA;EACA;EACAC;IACA;EACA;AACA","names":["name","data","options","movieInfo","movieData","dialogTitle","dialogFormVisible","form","methods","type","message","movieEdit","confirmButtonText","cancelButtonText","_id","addMovie","handleAvatarSuccess","beforeAvatarUpload","created"],"sourceRoot":"src/views/Home","sources":["MovieList.vue"],"sourcesContent":["<template>\r\n  <div id=\"movie-manage\">\r\n    <div class=\"top\">\r\n      <el-input  placeholder=\"请输入内容\" class=\"input-with-select\" style=\"width: 20%\">\r\n        <el-button slot=\"append\" icon=\"el-icon-search\">搜索</el-button>\r\n      </el-input>\r\n      <el-button type=\"primary\" plain style=\"margin-left: 25px\" @click=\"addMovie\">添加影片</el-button>\r\n\r\n    </div>\r\n\r\n    <div class=\"movie-table\">\r\n      <el-table\r\n          border\r\n          :data=\"movieInfo\">\r\n        <el-table-column\r\n            label=\"影片 ID\"\r\n            align=\"center\"\r\n            width=\"100\"\r\n            prop=\"_id\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"影片海报\"\r\n            align=\"center\"\r\n            prop=\"icon\">\r\n          <template v-slot=\"scope\">\r\n            <img :src=\"scope.row.icon\" height=\"200rem\" width=\"150rem\">\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"影片名称\"\r\n            show-overflow-tooltip\r\n            prop=\"name\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"主演\"\r\n            prop=\"actor\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"类型\"\r\n            align=\"center\"\r\n            width=\"100\"\r\n            prop=\"category\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"上映时间\"\r\n            prop=\"date\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            label=\"价格\"\r\n            prop=\"price\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            width=\"200\"\r\n            align=\"center\"\r\n            label=\"操作\">\r\n          <template v-slot=\"scope\">\r\n            <el-button\r\n                size=\"mini\"\r\n                @click=\"movieEdit(scope.$index,scope.row)\">\r\n              编辑\r\n            </el-button>\r\n            <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"remove(scope.$index,scope.row)\">\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n\r\n    <!--dialog-->\r\n    <div>\r\n      <el-dialog :title=\"dialogTitle\" :visible.sync=\"dialogFormVisible\" v-if=\"dialogFormVisible\"\r\n                 :showClose=\"false\" width=\"25%\">\r\n        <el-form :model=\"movieData\" @submit.native.prevent=\"save\">\r\n          <el-form-item label=\"影片名称\">\r\n            <el-input v-model=\"movieData.name\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"影片类别\" >\r\n            <el-select v-model=\"movieData.category\" multiple placeholder=\"请选择\" @focus=\"categoryFetch\">\r\n              <el-option\r\n                  v-for=\"item in options\"\r\n                  :key=\"item._id\"\r\n                  :value=\"item.name\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"影片海报:\" >\r\n            <el-upload\r\n                class=\"avatar-uploader\"\r\n                :action=\"$http.defaults.baseURL + '/upload/'\"\r\n                :show-file-list=\"false\"\r\n                :on-success=\"handleAvatarSuccess\"\r\n                :before-upload=\"beforeAvatarUpload\">\r\n              <img v-if=\"movieData.icon\" :src=\"movieData.icon\" class=\"avatar\">\r\n              <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n            </el-upload>\r\n          </el-form-item>\r\n          <el-form-item label=\"主演\" >\r\n            <el-input v-model=\"movieData.actor\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"价格\" >\r\n            <el-input v-model.number=\"movieData.price\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"上映日期\" >\r\n              <el-date-picker\r\n                  v-model=\"movieData.date\"\r\n                  type=\"datetime\"\r\n                  placeholder=\"选择日期时间\">\r\n              </el-date-picker>\r\n          </el-form-item>\r\n            <el-form-item label=\"影片介绍\" >\r\n              <el-input\r\n                  type=\"textarea\"\r\n                  autosize\r\n                  placeholder=\"请输入内容\"\r\n                  v-model=\"movieData.introduce\">\r\n              </el-input>\r\n            </el-form-item>\r\n\r\n<!--          </el-form-item>-->\r\n          <el-form-item>\r\n            <el-button type=\"primary\" native-type=\"submit\"\">保存</el-button>\r\n            <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n          </el-form-item>\r\n\r\n        </el-form>\r\n<!--        <div slot=\"footer\" class=\"dialog-footer\">-->\r\n<!--          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>-->\r\n<!--          <el-button type=\"primary\"-->\r\n<!--                     @click=\"save(movieInfo)\"-->\r\n<!--                     native-type=\"submit\">保存</el-button>-->\r\n<!--        </div>-->\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'MovieList',\r\n  data() {\r\n    return {\r\n      // imageURL: '',\r\n      options: [],\r\n      movieInfo: [],\r\n      movieData: {},\r\n      dialogTitle: '添加影片',\r\n      dialogFormVisible: false,\r\n      form: {\r\n        // name: '',\r\n        // region: '',\r\n        // date1: '',\r\n        // date2: '',\r\n        // delivery: false,\r\n        // type: [],\r\n        // resource: '',\r\n        // desc: ''\r\n      },\r\n    }\r\n\r\n\r\n  },\r\n  methods: {\r\n    //获取分类信息\r\n    async categoryFetch(){\r\n      const res = await this.$http.get('/category')\r\n      this.options = res.data\r\n    },\r\n    // 展示电影信息\r\n    async fetch() {\r\n      const res = await this.$http.get('/movie')\r\n      this.movieInfo = res.data\r\n    },\r\n    // 新增电影信息\r\n    async save() {\r\n      this.movieData.category = this.movieData.category.join(\",\")\r\n      //http://localhost:3000/admin/api/\r\n      const res = await this.$http.post('/movie', this.movieData)// eslint-disable-line no-unused-vars\r\n      //跳转路由\r\n      await this.$router.go(0)\r\n      this.$message({\r\n        type: 'success',\r\n        message: '添加成功',\r\n      })\r\n    },\r\n    //编辑按钮事件\r\n    movieEdit(index, row){\r\n      this.movieData = row\r\n      this.name = row.name\r\n      this.dialogFormVisible = true\r\n    },\r\n    //删除电信信息\r\n    async remove(index, row) {\r\n      this.$confirm(`此操作将删除电影信息\"${row.name}\", 是否继续?`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(async () => {\r\n        const res = await this.$http.delete(`/movie`, {data: {_id: row._id}}) // eslint-disable-line no-unused-vars\r\n        this.$message({\r\n          type: 'success',\r\n          message: '删除成功!'\r\n        });\r\n        await this.$router.go(0)\r\n      })\r\n    },\r\n    //显示dialog\r\n    addMovie() {\r\n      this.dialogTitle = '添加电影';\r\n      this.dialogFormVisible = true\r\n    },\r\n    handleAvatarSuccess(res) {\r\n      this.$set(this.movieData,'icon',res.url)\r\n      this.movieData.icon = res.url\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg';\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像图片只能是 JPG 格式!');\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 2MB!');\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n   },\r\n  // 自动执行\r\n  created() {\r\n    this.fetch()\r\n  },\r\n}\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.top {\r\n  display: flex;\r\n  margin-left:5%;\r\n  justify-content: center;\r\n  align-items:center;\r\n  padding: 30px 0;\r\n  margin-bottom: 30px;\r\n}\r\n.movie-table {\r\n  width: 90%;\r\n  min-width: 900px;\r\n  margin: 0 auto 30px;\r\n}\r\n\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 150px;\r\n  height: 200px;\r\n  line-height: 200px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 150px;\r\n  height: 200px;\r\n  display: block;\r\n}\r\n\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}