{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      currentPage: 1,\n      // 当前页码\n      total: 0,\n      // 总条数\n      pageSize: 5,\n      // 每页的数据条数\n      // 对话框显示标题\n      dialogTitle: \"\",\n      // 对话框显示状态\n      dialogFormVisible: false,\n      // 添加或编辑宣传信息\n      adInfo: {\n        name: \"\",\n        movieId: \"\",\n        adImage: \"\"\n      },\n      // 宣传列表数据\n      adList: [],\n      // 电影列表数据\n      movieList: [],\n      // 表单验证规则\n      // 搜索框内容\n      searchContent: \"\"\n    };\n  },\n  methods: {\n    //获取宣传\n    async fetch() {\n      const res = await this.$http.get('/advertise');\n      this.adList = res.data;\n    },\n    // 电影宣传海报上传成功\n\n    // 选中电影名后(得到是电影id),将电影名赋值给name\n    UpdateMovieName(val) {\n      this.adInfo.name = \"\";\n      // console.log(val);\n      this.movieList.some(item => {\n        if (item._id == val) {\n          this.adInfo.name = item.name;\n          return item._id == val;\n        }\n      });\n      console.log(this.adInfo);\n    },\n    // 获取电影列表\n    async movieFetch() {\n      const res = await this.$http.get('/movie');\n      this.movieList = res.data;\n    },\n    // 添加宣传\n    addMovie() {\n      // 清空排片列表\n      this.adInfo = {};\n      this.dialogTitle = \"添加广告\";\n      this.dialogFormVisible = true;\n    },\n    //确认对话框\n    // 取消对话框\n\n    // 编辑宣传\n    adEdit(row) {},\n    // 删除宣传\n    async adDelete(row) {\n      this.$confirm(`此操作将删除宣传信息\"${row.name}\", 是否继续?`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        const res = await this.$http.delete(`/register`, {\n          data: {\n            _id: row._id\n          }\n        }); // eslint-disable-line no-unused-vars\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        });\n        await this.$router.go(0);\n      });\n    },\n    // 新增宣传信息\n    async save() {\n      //http://localhost:3000/admin/api/\n      const res = await this.$http.post('/advertise', this.adInfo); // eslint-disable-line no-unused-vars\n      this.$message({\n        type: 'success',\n        message: '添加成功'\n      });\n      this.dialogFormVisible = false;\n      await this.$router.push('/home/movieAdvertise');\n    },\n    // 分页器当前页面变动时触发\n\n    handleAvatarSuccess(res) {\n      this.$set(this.adInfo, 'adImage', res.url);\n      this.adInfo.adImage = res.url;\n    },\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === 'image/jpeg';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n      return isJPG && isLt2M;\n    },\n    //每页条数改变时触发 选择一页显示多少行\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n      this.currentPage = 1;\n      this.pageSize = val;\n    },\n    //当前页改变时触发 跳转其他页\n    handleCurrentChange(val) {\n      console.log(`当前页: ${val}`);\n      this.currentPage = val;\n    }\n  },\n  created() {\n    this.fetch();\n    this.movieFetch();\n  }\n};","map":{"version":3,"mappings":";AAkHA;EACAA;IACA;MACAC;MAAA;MACAC;MAAA;MACAC;MAAA;MACA;MACAC;MACA;MACAC;MACA;MACAC;QACAC;QACAC;QACAC;MACA;MACA;MACAC;MACA;MACAC;MACA;MACA;MACAC;IACA;EACA;EACAC;IACA;IACA;MACA;MACA;IACA;IACA;;IAEA;IACAC;MACA;MACA;MACA;QACA;UACA;UACA;QACA;MACA;MACAC;IACA;IACA;IACA;MACA;MACA;IACA;IACA;IACAC;MACA;MACA;MACA;MACA;IACA;IACA;IACA;;IAEA;IACAC,aAEA;IACA;IACA;MACA;QACAC;QACAC;QACAC;MACA;QACA;UAAApB;YAAAqB;UAAA;QAAA;QACA;UACAD;UACAE;QACA;QACA;MACA;IACA;IACA;IACA;MACA;MACA;MACA;QACAF;QACAE;MACA;MACA;MACA;IACA;IACA;;IAEAC;MACA;MACA;IACA;IACAC;MACA;MACA;MAEA;QACA;MACA;MACA;QACA;MACA;MACA;IACA;IACA;IACAC;MACAV;MACA;MACA;IACA;IACA;IACAW;MACAX;MACA;IACA;EAEA;EACAY;IACA;IACA;EACA;AACA","names":["data","currentPage","total","pageSize","dialogTitle","dialogFormVisible","adInfo","name","movieId","adImage","adList","movieList","searchContent","methods","UpdateMovieName","console","addMovie","adEdit","confirmButtonText","cancelButtonText","type","_id","message","handleAvatarSuccess","beforeAvatarUpload","handleSizeChange","handleCurrentChange","created"],"sourceRoot":"src/views/Home","sources":["MovieAdvertise.vue"],"sourcesContent":["<template>\r\n  <div class=\"adPlan\">\r\n    <!--搜索-->\r\n    <div class=\"top\">\r\n      <el-input placeholder=\"请输入内容\" class=\"input-with-select\" style=\"width: 30%\">\r\n        <template v-slot:append>\r\n          <el-button icon=\"el-icon-search\">搜索</el-button>\r\n        </template>\r\n      </el-input>\r\n      <el-button type=\"primary\" size=\"small\" @click=\"addMovie\" style=\"margin-left: 15px;\">\r\n        添加宣传\r\n      </el-button>\r\n    </div>\r\n    <!-- 表格 -->\r\n    <div class=\"plan-table\">\r\n      <el-table border :data=\"adList.slice((currentPage-1)*pageSize,currentPage*pageSize)\">\r\n        <el-table-column\r\n            label=\"电影名\"\r\n            width=\"200\"\r\n            show-overflow-tooltip\r\n            prop=\"name\"\r\n            align=\"center\"\r\n        ></el-table-column>\r\n        <el-table-column label=\"宣传海报\" width=\"300\" align=\"center\">\r\n          <template slot-scope=\"scope\">\r\n            <img :src=\"scope.row.adImage\" style=\"height: 5rem; width: 100%\"/>\r\n          </template>\r\n        </el-table-column>\r\n        <!--        <el-table-column-->\r\n        <!--            label=\"上映时间\"-->\r\n        <!--            width=\"150\"-->\r\n        <!--            show-overflow-tooltip-->\r\n        <!--            prop=\"movieId.time\"-->\r\n        <!--            align=\"center\"-->\r\n        <!--        ></el-table-column>-->\r\n        <el-table-column align=\"center\" label=\"操作\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button size=\"mini\" @click=\"adEdit(scope.row)\"\r\n            >编辑\r\n            </el-button\r\n            >\r\n            <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"adDelete(scope.row)\"\r\n            >撤销\r\n            </el-button\r\n            >\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <!-- 分页器 -->\r\n    <el-pagination\r\n        align=\"center\"\r\n        small\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        layout=\"total,sizes,prev,pager,next,jumper\"\r\n        :total=\"adList.length\"\r\n        :page-size=\"pageSize\"\r\n        :page-sizes=\"[1,5,10]\">\r\n    </el-pagination>\r\n    <!-- 添加或编辑排片对话框 -->\r\n    <div>\r\n      <el-dialog\r\n          :title=\"dialogTitle\"\r\n          :visible.sync=\"dialogFormVisible\"\r\n          :modal-append-to-body=\"false\"\r\n          v-if=\"dialogFormVisible\"\r\n          :showClose=\"false\"\r\n          :close-on-click-modal=\"false\"\r\n          top=\"10vh\"\r\n      >\r\n        <el-form\r\n            label-width=\"150px\"\r\n            :model=\"adInfo\"\r\n            ref=\"planInfoForm\"\r\n        >\r\n          <el-form-item label=\"电影名\" prop=\"movieId\">\r\n              <el-select v-model=\"adInfo.movieId\" @change=\"UpdateMovieName\">\r\n                <el-option\r\n                    v-for=\"item in movieList\"\r\n                    :key=\"item._id\"\r\n                    :value=\"item._id\"\r\n                    :label=\"item.name\">\r\n                </el-option>\r\n              </el-select>\r\n\r\n          </el-form-item>\r\n          <el-form-item label=\"宣传海报\" prop=\"adImage\">\r\n              <el-upload\r\n                  class=\"avatar-uploader\"\r\n                  :action=\"$http.defaults.baseURL + '/upload/'\"\r\n                  :show-file-list=\"false\"\r\n                  :on-success=\"handleAvatarSuccess\"\r\n                  :before-upload=\"beforeAvatarUpload\">\r\n                <img v-if=\"adInfo.adImage\" :src=\"adInfo.adImage\" class=\"avatar\">\r\n                <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n              </el-upload>\r\n\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      currentPage: 1, // 当前页码\r\n      total: 0, // 总条数\r\n      pageSize: 5,// 每页的数据条数\r\n      // 对话框显示标题\r\n      dialogTitle: \"\",\r\n      // 对话框显示状态\r\n      dialogFormVisible: false,\r\n      // 添加或编辑宣传信息\r\n      adInfo: {\r\n        name: \"\",\r\n        movieId: \"\",\r\n        adImage: \"\",\r\n      },\r\n      // 宣传列表数据\r\n      adList: [],\r\n      // 电影列表数据\r\n      movieList: [],\r\n      // 表单验证规则\r\n      // 搜索框内容\r\n      searchContent: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    //获取宣传\r\n    async fetch() {\r\n      const res = await this.$http.get('/advertise')\r\n      this.adList = res.data\r\n    },\r\n    // 电影宣传海报上传成功\r\n\r\n    // 选中电影名后(得到是电影id),将电影名赋值给name\r\n    UpdateMovieName(val) {\r\n      this.adInfo.name = \"\";\r\n      // console.log(val);\r\n      this.movieList.some((item) => {\r\n        if (item._id == val) {\r\n          this.adInfo.name = item.name;\r\n          return item._id == val;\r\n        }\r\n      });\r\n      console.log(this.adInfo);\r\n    },\r\n    // 获取电影列表\r\n    async movieFetch() {\r\n      const res = await this.$http.get('/movie')\r\n      this.movieList = res.data\r\n    },\r\n    // 添加宣传\r\n    addMovie() {\r\n      // 清空排片列表\r\n      this.adInfo = {};\r\n      this.dialogTitle = \"添加广告\";\r\n      this.dialogFormVisible = true;\r\n    },\r\n    //确认对话框\r\n    // 取消对话框\r\n\r\n    // 编辑宣传\r\n    adEdit(row){\r\n\r\n    },\r\n    // 删除宣传\r\n    async adDelete(row){\r\n      this.$confirm(`此操作将删除宣传信息\"${row.name}\", 是否继续?`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(async () => {\r\n        const res = await this.$http.delete(`/register`, {data: {_id: row._id}}) // eslint-disable-line no-unused-vars\r\n        this.$message({\r\n          type: 'success',\r\n          message: '删除成功!'\r\n        });\r\n        await this.$router.go(0)\r\n      })\r\n    },\r\n    // 新增宣传信息\r\n    async save() {\r\n      //http://localhost:3000/admin/api/\r\n      const res = await this.$http.post('/advertise', this.adInfo)// eslint-disable-line no-unused-vars\r\n      this.$message({\r\n        type: 'success',\r\n        message: '添加成功',\r\n      })\r\n      this.dialogFormVisible = false\r\n      await this.$router.push('/home/movieAdvertise')\r\n    },\r\n    // 分页器当前页面变动时触发\r\n\r\n    handleAvatarSuccess(res) {\r\n      this.$set(this.adInfo, 'adImage', res.url)\r\n      this.adInfo.adImage = res.url\r\n    },\r\n    beforeAvatarUpload(file) {\r\n      const isJPG = file.type === 'image/jpeg';\r\n      const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n      if (!isJPG) {\r\n        this.$message.error('上传头像图片只能是 JPG 格式!');\r\n      }\r\n      if (!isLt2M) {\r\n        this.$message.error('上传头像图片大小不能超过 2MB!');\r\n      }\r\n      return isJPG && isLt2M;\r\n    },\r\n    //每页条数改变时触发 选择一页显示多少行\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n      this.currentPage = 1;\r\n      this.pageSize = val;\r\n    },\r\n    //当前页改变时触发 跳转其他页\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.currentPage = val;\r\n    },\r\n\r\n  },\r\n  created() {\r\n    this.fetch()\r\n    this.movieFetch()\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.top {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  padding: 30px 0;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.adPlan {\r\n  width: 90%;\r\n  min-width: 900px;\r\n  margin: 0 auto 30px;\r\n}\r\n\r\n// 海报图片样式\r\n.avatar-uploader-icon {\r\n  width: 276px;\r\n  height: 92px;\r\n  line-height: 92px;\r\n}\r\n\r\n.avatar {\r\n  width: 276px;\r\n  height: 92px;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}