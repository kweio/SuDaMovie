<el-table-column prop=\"name\" label=\"类别名称\"></el-table-column>\r\n        <el-table-column\r\n            width=\"200\"\r\n            align=\"center\"\r\n            label=\"操作\">\r\n          <template v-slot=\"scope\">\r\n\r\n            <el-button\r\n                size=\"mini\"\r\n                @click=\"categoryEdit(scope.row)\"\r\n            >\r\n              编辑\r\n            </el-button>\r\n            <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"remove(scope.row)\">\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <!--    编辑表单-->\r\n    <div>\r\n      <el-dialog title=\"编辑类别\" :visible.sync=\"dialogFormVisible\">\r\n        <el-form>\r\n          <el-form-item label=\"类别名称\" label-width=\"100px\" prop=\"name\">\r\n            <el-input v-model=\"hallInfo.name\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"editSave(hallInfo)\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n    <!--分页-->\r\n    <el-pagination\r\n        align=\"center\"\r\n        small\r\n        @size-change = \"handleSizeChange\"\r\n        @current-change = \"handleCurrentChange\"\r\n        :current-page=\"currentPage\"\r\n        layout=\"total,sizes,prev,pager,next,jumper\"\r\n        :total=\"items.length\"\r\n        :page-size=\"pageSize\"\r\n        :page-sizes=\"[1,5,10]\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// import Page from \"@/components/Page.vue\";\r\nexport default {\r\n  name: \"MovieHall\",\r\n  // components: {Page},\r\n  props: {\r\n    id:{},\r\n  },\r\n  data() {\r\n    return {\r\n      currentPage: 1, // 当前页码\r\n      total: 20, // 总条数\r\n      pageSize: 5 ,// 每页的数据条数\r\n      model: {},\r\n      items: [],\r\n      hallInfo: {},\r\n      form: {\r\n        name: '',\r\n      },\r\n      dialogFormVisible: false,\r\n    }\r\n\r\n\r\n  },\r\n\r\n  methods: {\r\n    //保存新增分类\r\n    async addCategory() {\r\n      //http://localhost:3000/admin/api/\r\n      const res = await this.$http.post('hall', this.model)// eslint-disable-line no-unused-vars\r\n      //跳转路由\r\n      await this.$router.go(0)\r\n      this.$message({\r\n        type: 'success',\r\n        message: '新增成功',\r\n      })\r\n    },\r\n    //删除类别\r\n    async remove(row) {\r\n      this.$confirm(`此操作将删除分类\"${row.name}\", 是否继续?`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(async () => {\r\n        const res = await this.$http.delete(`/hall`, {data: {_id: row._id}}) // eslint-disable-line no-unused-vars\r\n        this.$message({\r\n          type: 'success',\r\n          message: '删除成功!'\r\n        });\r\n        await this.$router.go(0)\r\n      })\r\n    },\r\n    //编辑类别\r\n    categoryEdit(row) {\r\n      this.hallInfo = row\r\n      this.name = row.name\r\n      this.dialogFormVisible = true\r\n    },\r\n    //修改保存\r\n    async editSave() {\r\n      const res = await this.$http.put('/hall', this.hallInfo)// eslint-disable-line no-unused-vars\r\n      await this.$router.go(0)\r\n      this.$message({\r\n        type: 'success',\r\n        message: '修改成功',\r\n      })\r\n    },\r\n    //查找分类\r\n    async fetch() {\r\n      const res = await this.$http.get('/hall')\r\n      this.items = res.data\r\n    },\r\n    //每页条数改变时触发 选择一页显示多少行\r\n    handleSizeChange(val) {\r\n      console.log(`每页 ${val} 条`);\r\n      this.currentPage = 1;\r\n      this.pageSize = val;\r\n    },\r\n    //当前页改变时触发 跳转其他页\r\n    handleCurrentChange(val) {\r\n      console.log(`当前页: ${val}`);\r\n      this.currentPage = val;\r\n    },\r\n    // initData(){\r\n    //   const dataA = {}\r\n    //   for(let k in this.$refs[\"childData\"]){\r\n    //     dataA[k] = this.$refs[\"childData\"][k]\r\n    //   }\r\n    //  return dataA\r\n    // },\r\n  },\r\n  //自动执行\r\n  created() {\r\n    this.fetch()\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.personal_top,\r\n.personal_ctn {\r\n  .el-button--primary:focus,\r\n  .el-button--primary:hover {\r\n    background: #009c7f;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}