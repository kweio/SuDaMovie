{"ast":null,"code":"// import Page from \"@/components/Page.vue\";\nexport default {\n  name: \"MovieHall\",\n  // components: {Page},\n  props: {\n    id: {}\n  },\n  data() {\n    return {\n      currentPage: 1,\n      // 当前页码\n      total: 20,\n      // 总条数\n      pageSize: 5,\n      // 每页的数据条数\n      model: {},\n      items: [],\n      hallInfo: {},\n      form: {\n        name: ''\n      },\n      dialogFormVisible: false\n    };\n  },\n  methods: {\n    //保存新增分类\n    async addCategory() {\n      //http://localhost:3000/admin/api/\n      const res = await this.$http.post('hall', this.model); // eslint-disable-line no-unused-vars\n      //跳转路由\n      await this.$router.go(0);\n      this.$message({\n        type: 'success',\n        message: '新增成功'\n      });\n    },\n    //删除类别\n    async remove(row) {\n      this.$confirm(`此操作将删除分类\"${row.name}\", 是否继续?`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        const res = await this.$http.delete(`/hall`, {\n          data: {\n            _id: row._id\n          }\n        }); // eslint-disable-line no-unused-vars\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        });\n        await this.$router.go(0);\n      });\n    },\n    //编辑类别\n    categoryEdit(row) {\n      this.hallInfo = row;\n      this.name = row.name;\n      this.dialogFormVisible = true;\n    },\n    //修改保存\n    async editSave() {\n      const res = await this.$http.put('/hall', this.hallInfo); // eslint-disable-line no-unused-vars\n      await this.$router.go(0);\n      this.$message({\n        type: 'success',\n        message: '修改成功'\n      });\n    },\n    //查找分类\n    async fetch() {\n      const res = await this.$http.get('/hall');\n      this.items = res.data;\n    },\n    //每页条数改变时触发 选择一页显示多少行\n    handleSizeChange(val) {\n      console.log(`每页 ${val} 条`);\n      this.currentPage = 1;\n      this.pageSize = val;\n    },\n 