{"ast":null,"code":"export default {\n  name: \"MovieCategory\",\n  data() {\n    return {\n      model: {},\n      items: [],\n      categoryInfo: {},\n      form: {\n        name: ''\n      },\n      dialogFormVisible: false\n    };\n  },\n  methods: {\n    //保存新增分类\n    async save() {\n      console.log(\"开始执行save\");\n      //http://localhost:3000/admin/api/\n      const res = await this.$http.post('http://localhost:3000/admin/api/category', this.model); // eslint-disable-line no-unused-vars\n      //跳转路由\n      await this.$router.go(0);\n      this.$message({\n        type: 'success',\n        message: '保存成功'\n      });\n    },\n    //查找分类\n    async fetch() {\n      const res = await this.$http.get('http://localhost:3000/admin/api/category');\n      this.items = res.data;\n    },\n    async manageCategoryInfo() {\n      console.log(this.categoryInfo);\n    },\n    edit() {\n      this.dialogFormVisible = true;\n    },\n    async remove(row) {\n      this.$confirm(`此操作将删除分类\"$(row.name)\", 是否继续?`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        const res = await this.$http.delete(`http://localhost:3000/admin/api/category/$(row._id)`); // eslint-disable-line no-unused-vars\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        });\n        await this.$router.go(0);\n      });\n    }\n  },\n  //自动执行\n  created() {\n    this.fetch();\n  }\n};","map":{"version":3,"mappings":"AAyDA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;QACAL;MACA;MACAM;IACA;EAGA;EACAC;IACA;IACA;MACAC;MACA;MACA;MACA;MACA;MACA;QACAC;QACAC;MACA;IACA;IACA;IACA;MACA;MACA;IACA;IACA;MACAF;IACA;IACAG;MACA;IACA;IACA;MACA;QACAC;QACAC;QACAJ;MACA;QACA;QACA;UACAA;UACAC;QACA;QACA;MACA;IACA;EACA;EACA;EACAI;IACA;EAEA;AACA","names":["name","data","model","items","categoryInfo","form","dialogFormVisible","methods","console","type","message","edit","confirmButtonText","cancelButtonText","crea