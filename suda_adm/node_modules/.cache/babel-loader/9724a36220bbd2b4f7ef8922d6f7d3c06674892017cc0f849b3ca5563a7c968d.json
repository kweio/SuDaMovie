{"ast":null,"code":"import { mapActions } from \"vuex\";\nimport { mapState } from \"vuex\";\nimport Pag from \"../../components/Page.vue\";\nexport default {\n  name: \"MovieCategory\",\n  computed: {\n    ...mapState(\"personalAbout\", {\n      //获取state的数据\n      psldata: \"data\",\n      total: \"total\",\n      tableData: \"tableData\"\n    })\n  },\n  components: {\n    Pag\n  },\n  props: {\n    id: {}\n  },\n  data() {\n    return {\n      model: {},\n      items: [],\n      categoryInfo: {},\n      form: {\n        name: ''\n      },\n      dialogFormVisible: false\n    };\n  },\n  methods: {\n    //保存新增分类\n    async addCategory() {\n      //http://localhost:3000/admin/api/\n      const res = await this.$http.post('category', this.model); // eslint-disable-line no-unused-vars\n      //跳转路由\n      await this.$router.go(0);\n      this.$message({\n        type: 'success',\n        message: '新增成功'\n      });\n    },\n    //删除类别\n    async remove(index, row) {\n      this.$confirm(`此操作将删除分类\"${row.name}\", 是否继续?`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(async () => {\n        const res = await this.$http.delete(`/category`, {\n          data: {\n            _id: row._id\n          }\n        }); // eslint-disable-line no-unused-vars\n        this.$message({\n          type: 'success',\n          message: '删除成功!'\n        });\n        await this.$router.go(0);\n      });\n    },\n    //编辑类别\n    categoryEdit(index, row) {\n      this.categoryInfo = row;\n      this.name = row.name;\n      this.dialogFormVisible = true;\n    },\n    async editSave() {\n      const res = await this.$http.put('/category', this.categoryInfo); // eslint-disable-line no-unused-vars\n      await this.$router.go(0);\n      this.$message({\n        type: 'success',\n        message: '修改成功'\n      });\n    },\n    // handleSizeChange(val) {\n    //   console.log(`每页 ${val} 条`);\n    //   this.currentPage = 1;\n    //   this.pageSize = val;\n    // },\n    // //当前页改变时触发 跳转其他页\n    // handleCurrentChange(val) {\n    //   console.log(`当前页: ${val}`);\n    //   this.currentPage = val;\n    // },\n    //查找分类\n    async fetch() {\n      const res = await this.$http.get('/category');\n      this.items = res.data;\n    }\n  },\n  //分页\n  //Actions使用axios\n  ...mapActions(\"personalAbout\", {\n    getTableData: \"getTableData\"\n  }),\n  //获取pag的页码\n  getPage(page) {\n    this.getTableData(page);\n  },\n  //自动执行\n  created() {\n    this.fetch();\n  },\n  mounted() {\n    //  dom更新完的钩子，//初始化传值\n    this.getTableData(1);\n  }\n};","map":{"version":3,"mappings":"AAiEA;AACA;AACA;AACA;EAEAA;EACAC;IACA;MAAA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;EACA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;QACAb;MACA;MACAc;IACA;EAGA;EAEAC;IACA;IACA;MACA;MACA;MACA;MACA;MACA;QACAC;QACAC;MACA;IACA;IACA;IACA;MACA;QACAC;QACAC;QACAH;MACA;QACA;UAAAP;YAAAW;UAAA;QAAA;QACA;UACAJ;UACAC;QACA;QACA;MACA;IACA;IACA;IACAI;MACA;MACA;MACA;IACA;IACA;MACA;MACA;MACA;QACAL;QACAC;MACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;MACA;MACA;IACA;EACA;EACA;EACA;EACA;IAAAK;EAAA;EACA;EACAC;IACA;EACA;EACA;EACAC;IACA;EACA;EACAC;IACA;IACA;EACA;AACA","names":["name","computed","psldata","total","tableData","components","Pag","props","id","data","model","items","categoryInfo","form","dialogFormVisible","methods","type","message","confirmButtonText","cancelButtonText","_id","categoryEdit","getTableData","getPage","created","mounted"],"sourceRoot":"src/views/Home","sources":["MovieCategory.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"addCategory\">\r\n      <el-form @submit.native.prevent=\"addCategory\">\r\n        <h1>新建类别</h1>\r\n        <el-form-item label=\"名称\">\r\n          <el-input style=\"width: 150px\" v-model=\"model.name\"></el-input>\r\n          <el-button style=\"margin-left: 15px\" type=\"primary\" native-type=\"submit\">确定</el-button>\r\n        </el-form-item>\r\n      </el-form>\r\n    </div>\r\n    <div class=\"categoryList\">\r\n      <h1>类别列表</h1>\r\n      <el-table :data=\"items\">\r\n        <el-table-column prop=\"_id\" label=\"ID\" width=\"300px\"></el-table-column>\r\n        <el-table-column prop=\"name\" label=\"类别名称\"></el-table-column>\r\n        <el-table-column\r\n            width=\"200\"\r\n            align=\"center\"\r\n            label=\"操作\">\r\n          <template v-slot=\"scope\">\r\n\r\n            <el-button\r\n                size=\"mini\"\r\n                @click=\"categoryEdit(scope.$index,scope.row)\"\r\n            >\r\n              编辑\r\n            </el-button>\r\n            <el-button\r\n                size=\"mini\"\r\n                type=\"danger\"\r\n                @click=\"remove(scope.$index,scope.row)\">\r\n              删除\r\n            </el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    <!--    编辑表单-->\r\n    <div>\r\n      <el-dialog title=\"编辑类别\" :visible.sync=\"dialogFormVisible\">\r\n        <el-form>\r\n          <el-form-item label=\"类别名称\" label-width=\"100px\" prop=\"name\">\r\n            <el-input v-model=\"categoryInfo.name\" autocomplete=\"off\"></el-input>\r\n          </el-form-item>\r\n        </el-form>\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n          <el-button type=\"primary\" @click=\"editSave(categoryInfo)\">确 定</el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n    <!--分页-->\r\n    <div class=\"personer_foot\">\r\n      <Pag\r\n          :pageSize=\"psldata.pageSize\"\r\n          :page=\"psldata.page\"\r\n          :total=\"total\"\r\n          @current-change=\"getPage\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapActions} from \"vuex\";\r\nimport {mapState} from \"vuex\";\r\nimport Pag from \"../../components/Page.vue\";\r\nexport default {\r\n\r\n  name: \"MovieCategory\",\r\n  computed: {\r\n    ...mapState(\"personalAbout\", {  //获取state的数据\r\n      psldata: \"data\",\r\n      total: \"total\",\r\n      tableData: \"tableData\",\r\n    }),\r\n  },\r\n  components: {\r\n    Pag,\r\n  },\r\n  props: {\r\n    id:{},\r\n  },\r\n  data() {\r\n    return {\r\n      model: {},\r\n      items: [],\r\n      categoryInfo: {},\r\n      form: {\r\n        name: '',\r\n      },\r\n      dialogFormVisible: false,\r\n    }\r\n\r\n\r\n  },\r\n\r\n  methods: {\r\n    //保存新增分类\r\n    async addCategory() {\r\n      //http://localhost:3000/admin/api/\r\n      const res = await this.$http.post('category', this.model)// eslint-disable-line no-unused-vars\r\n      //跳转路由\r\n      await this.$router.go(0)\r\n      this.$message({\r\n        type: 'success',\r\n        message: '新增成功',\r\n      })\r\n    },\r\n    //删除类别\r\n    async remove(index, row) {\r\n      this.$confirm(`此操作将删除分类\"${row.name}\", 是否继续?`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(async () => {\r\n        const res = await this.$http.delete(`/category`, {data: {_id: row._id}}) // eslint-disable-line no-unused-vars\r\n        this.$message({\r\n          type: 'success',\r\n          message: '删除成功!'\r\n        });\r\n        await this.$router.go(0)\r\n      })\r\n    },\r\n    //编辑类别\r\n    categoryEdit(index, row) {\r\n      this.categoryInfo = row\r\n      this.name = row.name\r\n      this.dialogFormVisible = true\r\n    },\r\n    async editSave() {\r\n      const res = await this.$http.put('/category', this.categoryInfo)// eslint-disable-line no-unused-vars\r\n      await this.$router.go(0)\r\n      this.$message({\r\n        type: 'success',\r\n        message: '修改成功',\r\n      })\r\n    },\r\n    // handleSizeChange(val) {\r\n    //   console.log(`每页 ${val} 条`);\r\n    //   this.currentPage = 1;\r\n    //   this.pageSize = val;\r\n    // },\r\n    // //当前页改变时触发 跳转其他页\r\n    // handleCurrentChange(val) {\r\n    //   console.log(`当前页: ${val}`);\r\n    //   this.currentPage = val;\r\n    // },\r\n    //查找分类\r\n    async fetch() {\r\n      const res = await this.$http.get('/category')\r\n      this.items = res.data\r\n    },\r\n  },\r\n  //分页\r\n  //Actions使用axios\r\n  ...mapActions(\"personalAbout\", { getTableData: \"getTableData\" }),\r\n  //获取pag的页码\r\n  getPage(page) {\r\n    this.getTableData(page);\r\n  },\r\n  //自动执行\r\n  created() {\r\n    this.fetch()\r\n  },\r\n  mounted() {\r\n    //  dom更新完的钩子，//初始化传值\r\n    this.getTableData(1);\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.personal_top,\r\n.personal_ctn {\r\n  .el-button--primary:focus,\r\n  .el-button--primary:hover {\r\n    background: #009c7f;\r\n  }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}